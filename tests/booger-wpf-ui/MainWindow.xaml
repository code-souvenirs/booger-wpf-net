<Window x:Class="Jsinh.BoogerWpf.Test.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:viewModel="clr-namespace:Jsinh.BoogerWpf.Test.ViewModel"
        Title="MainWindow"
        Height="350"
        Width="525">
    <Window.DataContext>
        <Binding Source="{StaticResource Locator}"
                 Path="Main" />
    </Window.DataContext>
    <Grid>
        <ComboBox Width="200" Height="30">
            <ComboBox.ItemsSource>
                <Binding Source="{x:Type viewModel:TestEnum}" Converter="{StaticResource TestingKey}"></Binding>
            </ComboBox.ItemsSource>
        </ComboBox>
        <!--<TextBox Text="{Binding SomeBoolProperty, Converter={StaticResource StringFormatConverter}}" />-->
        <!--<Button Width="90"
                Height="25"
                Content="Change"
                Command="{Binding ChangeValueCommand}" />-->
    </Grid>
</Window>